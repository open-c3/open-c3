#!/usr/bin/env /data/Software/mydan/python3/bin/python3
# -*- coding: utf-8 -*-

import json


def get_data_list():
    """
        按量计费实例关机收费模式
    """
    return [
        {
            "id": "KEEP_CHARGING",
            "desc": "关机继续收费"
        },
        {
            "id": "STOP_CHARGING",
            "desc": "关机停止收费"
        }
    ]


def main():
    data_list = get_data_list()
    for data in data_list:
        print(json.dumps(data))


if __name__ == '__main__':
    main()
